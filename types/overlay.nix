# ============================================================================ #
#
# Nixpkgs Overlay.
# Adds FS types to `lib.ytypes'.
# Only depends on `ytypes.{Prim,Core}'.
#
# ---------------------------------------------------------------------------- #

final: prev: {

  lib = prev.lib.extend ( _: prevLib: {
    ytypes = prevLib.ytypes // {
      FS = import ./fs.nix { inherit (prevLib) ytypes; };
    };
  } );

}


